<!--[if gt IE 8]><!--> <html class="no-js"> <!--<![endif]-->
    <head>
        <meta charset="utf-8">

        <title>{{ repository.full_name }} - gitscore</title>
        <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no" />
        <meta name="description" content="Developer ranking from your github project" />
        <link rel="stylesheet" href="/assets/dev/css/app.css" />
    </head>
    <body>
        <h1 style="text-align:center">{{ repository.full_name }} - {{ user.login }}</h1>

        <img src="{{ user.avatar_url }}" width="48px" height="48px" />

        <ul>
            {% for pull in pulls %}
                <li>
                    <a href="{{ pull.html_url }}">{{ pull.title }}</a>
                </li>
            {% endfor %}
        </ul>
    </body>
</html>